{
  "name": "@socialjitsu/types",
  "version": "1.0.48",
  "description": "SocialJitsu TS typings",
  "keywords": [
    "typescript"
  ],
  "homepage": "https://github.com/socialjitsu/sdk/blob/master/packages/types/README.md",
  "bugs": "https://github.com/socialjitsu/socialjitsu/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socialjitsu/tools.git"
  },
  "license": "MIT",
  "author": "SocialJitsu (https://socialjitsu.com/)",
  "type": "module",
  "scripts": {
    "package": "yarn npm publish",
    "build": "run -T tsc --build tsconfig.build.json --force",
    "clean": "tsc --build tsconfig.build.json --clean",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "wipe": "del-cli dist",
    "types": "cpy $TYPES_SRC/models/* ./src/models/ --flat  && cpy $TYPES_SRC/graphql/public.ts ./src/graphql/ --rename=index.ts --flat"
  },
  "devDependencies": {
    "cpy-cli": "^4.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "cb2b0883e2893cac03aaeabdda5760936f36baa6",
  "dependencies": {
    "graphql": "^16.8.1"
  }
}
